	"description": "運営と運命を共にする、うんち実況（セーラージュピター）",
	"main": "index.js",
	"scripts": {
		"clean": "rimraf dist",
		"build": "esbuild src/server/index.ts --bundle --platform=node --format=cjs --outfile=dist/server.js",
		"obfuscate": "javascript-obfuscator dist --output dist --config obfuscate.json",
		"dev": "pnpm run clean && tsx workflow/copy.ts && cross-env DEV_MODE=true vite build && cross-env DEV_MODE=true tsx src/server/index.ts",
		"stg": "pnpm run clean && tsx workflow/copy.ts && cross-env STG_MODE=true vite build && pnpm run build && pnpm run obfuscate && tsx workflow/rename.ts && cross-env STG_MODE=true node dist/server.cjs",
		"prod": "pnpm run clean && tsx workflow/copy.ts && vite build && pnpm run build && pnpm run obfuscate && tsx workflow/rename.ts",
		"sync": "tsx workflow/sync.ts",
		"start": "tsx src/server/index.ts",
		"cf": "cd ./uploader-worker && pnpm pnpx wrangler deploy",
		"cft": "cd ./uploader-worker && pnpm pnpx wrangler tail uploader-worker"
	},
