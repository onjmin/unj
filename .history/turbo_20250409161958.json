{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "dev": {
            "dependsOn": [
                "start"
            ]
        },
        "prod": {
            "dependsOn": [
                "start"
            ]
        },
        "rpgen-map#build": {
            "outputs": [
                "dist/**"
            ]
        },
        "start": {
            "dependsOn": [
                "rpgen-map#build"
            ],
            "persistent": true
        }
    },
    "ui": "tui"
}